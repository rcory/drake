<?xml version="1.0" encoding="utf-8"?>
<sdf version="1.6" xmlns:drake="drake.mit.edu">
  <model name="Schunk_Gripper" xmlns:drake="drake.mit.edu">
    <link name="base">
      <inertial>
        <mass>0.85</mass>
        <pose>0 0 0 0 0 0 </pose>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy>
          <iyz>0.0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name="base_visual">
        <pose>0 0 0.0 0 0 0</pose>
        <geometry>
          <capsule>
            <radius>.001</radius>
            <length>.055</length>
          </capsule>
        </geometry>
        <material>
          <diffuse>0.5 0.5 0.5 1</diffuse>
        </material>
      </visual>
    </link>
    <link name="finger_slider">
      <pose relative_to="left_finger">0.01 0 -0.105 0 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <pose>0 0 0 0 0 0 </pose>
        <inertia>
          <ixx>1e-6</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>1e-6</iyy>
          <iyz>0.0</iyz>
          <izz>1e-6</izz>
        </inertia>
      </inertial>
      <visual name="finger_slider_visual">
        <pose>0 0 0 0 1.57 0</pose>
        <geometry>
          <cylinder>
            <radius>.005</radius>
            <length>.005</length>
          </cylinder>
        </geometry>
        <material>
          <diffuse>0.1 0.7 0.1 1</diffuse>
        </material>
      </visual>
    </link>
    <joint name="finger_slider_joint" type="prismatic">
      <child>finger_slider</child>
      <parent>base</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0.0</lower>
          <upper>0.055</upper>
          <effort>0</effort>
        </limit>
      </axis>
    </joint>
    <link name="left_finger">
      <pose relative_to="base">-0.01 0 0.08 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>1.0e-06</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>1.0e-06</iyy>
          <iyz>0.0</iyz>
          <izz>1.0e-6</izz>
        </inertia>
      </inertial>
      <visual name="left_finger_l1_visual">
        <pose>0 -0.025 0 0 0 0</pose>
        <geometry>
          <box>
            <size>.01 0.05 .01</size>
          </box>
        </geometry>
        <material>
          <diffuse>0.7 0.2 0.2 0.7</diffuse>
        </material>
      </visual>
    </link>
    <link name="left_finger_pushrod">
      <pose relative_to="finger_slider">0 0 0 0 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <pose>0 0 0 0 0 0 </pose>
        <inertia>
          <ixx>1e-6</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>1e-6</iyy>
          <iyz>0.0</iyz>
          <izz>1e-6</izz>
        </inertia>
      </inertial>
      <visual name="left_finger_pushrod_visual">
        <pose>0 0 0.0581485167481 0 0 0</pose>
        <geometry>
          <box>
            <size>0.01 0.01 0.116297033496</size>
          </box>
        </geometry>
        <material>
          <diffuse>0.2 0.2 0.7 0.7</diffuse>
        </material>
      </visual>
    </link>
    <joint name="left_finger_joint" type="prismatic">
      <child>left_finger</child>
      <parent>base</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.055</lower>
          <upper>0.0</upper>
          <effort>1.0</effort>
        </limit>
      </axis>
    </joint>
    <joint name="left_finger_pushrod_joint" type="revolute">
      <child>left_finger_pushrod</child>
      <parent>finger_slider</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <effort>0</effort>
        </limit>
      </axis>
    </joint>
    <frame attached_to="left_finger" name="frameA">
      <pose relative_to="left_finger">0 -0.05 0 0 -1.57079632679 0</pose>
    </frame>
    <frame attached_to="left_finger_pushrod" name="frameC">
      <pose relative_to="left_finger_pushrod">0 0 0.116297033496 0 -1.57079632679 0</pose>
    </frame>
    <drake:linear_bushing_rpy>
      <drake:bushing_frameA>frameA</drake:bushing_frameA>
      <drake:bushing_frameC>frameC</drake:bushing_frameC>
      <drake:bushing_torque_stiffness>0 0 0</drake:bushing_torque_stiffness>
      <drake:bushing_torque_damping>0 0 0</drake:bushing_torque_damping>
      <drake:bushing_force_stiffness>7000 7000 7000</drake:bushing_force_stiffness>
      <drake:bushing_force_damping>0 0 0</drake:bushing_force_damping>
    </drake:linear_bushing_rpy>
  </model>
</sdf>

