<?xml version="1.0"?>
<sdf version="1.7">
  <model name="ikea_dinera_plate_8in">
    <!--
    This frame has its +z axis pointing up (sticking out of the food).
    The origin-point of the plate-frame is along the plate's symmetry axis.
    If the plate was placed on a flat table, the origin (x=0, y=0, z=0) would
    be touching the table.  All particles in the plate have a z-value greater
    than or equal to zero.  (Note that due to rounding errors a few vertices
    are approximately a micron below zero.)
    -->
    <link name="ikea_dinera_plate_8in">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0.00915 0 0 0</pose>
        <mass>0.375</mass>
        <inertia>
          <ixx>0.000860</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000860</iyy>
          <iyz>0</iyz>
          <izz>0.00171</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>ikea_dinera_plate_8in_500.obj</uri>
            <scale>1.0 1.0 1.0</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name="collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>ikea_dinera_plate_8in_500.obj</uri>
            <scale>1.0 1.0 1.0</scale>
          </mesh>
        </geometry>
        <drake:proximity_properties>
          <drake:rigid_hydroelastic/>
          <drake:mesh_resolution_hint>0.1</drake:mesh_resolution_hint>
        </drake:proximity_properties>
        <material>
          <drake:diffuse_map>ikea_dinera_plate_8in_color.png</drake:diffuse_map>
        </material>
      </collision>
    </link>
    <frame name="flush_bottom_center__z_up">
      <pose relative_to="ikea_dinera_plate_8in">0 0 0 0 0 0</pose>
    </frame>
    <frame name="origin">
      <pose relative_to="ikea_dinera_plate_8in">0 0 0 0 0 0</pose>
    </frame>
  </model>
</sdf>
